{
  "name": "bs-vite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "vite build && rm -rf node_modules",
    "watch": "vite build --watch",
    "preview": "npm-run-all --parallel sync watch",
    "clean": "sudo su -c 'rm -rf node_modules && rm -f yarn.lock'",
    "restart": "yarn clean && yarn install && yarn dev"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "browser-sync": "^2.27.11",
    "cross-env": "^7.0.3",
    "crypto": "npm:crypto-browserify@3.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-nested": "^6.0.0",
    "sass": "^1.58.0",
    "vite": "^4.0.1",
    "vite-plugin-chunk-split": "^0.4.7",
    "vite-plugin-live-reload": "^3.0.1"
  },
  "dependencies": {
    "@vue-leaflet/vue-leaflet": "^0.6.0",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vuepic/vue-datepicker": "^4.2.1",
    "@vueuse/core": "^9.13.0",
    "gsap": "^3.5.1",
    "lazysizes": "^5.3.2",
    "leaflet": "^1.9.3",
    "typescript": "^5.0.4",
    "vanilla-lazyload": "latest",
    "vue": "^3.2.45",
    "vue3-carousel": "^0.2.9",
    "vue-currency-input": "^3.0.3",
    "vue-tsc": "^1.8.18"
  },
  "engines": {
    "node": "18.19.0"
  }
}

