extends template

block css

block title

  title Наши работы


block content

  include ../includes/page-header-photo.pug

  main.page-main
    include ../includes/after-header.pug
    section.our-works
      .container
        header.page-form__header.section-header
          h3.page-form__title.section-header__title Наши работ
          a.page-form__button.button.button--small.section-header__button(href="index.html") На главную
        //section-header

        .our-works__list
          .our-works__item.our-works__item--month-work
            .our-works__picture
              picture
                source(media="(min-width: 768px)", srcset="assets/i/our-works/1-tablet.jpg")
                img.our-works__image(src="assets/i/our-works/1-mobile.jpg" alt="")
              .our-works__label работа месяца
            .our-works__content
              h3.our-works__title Лжепётр Мчиславский
              p.our-works__text Где я только не стригся, но так как делаете это вы — не умеет никто другой! Еще раз спасибо и до скорого!
              ul.our-works__meta.meta
                li.meta__item
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="21" height="20" viewBox="0 0 21 20"><g><g transform="translate(-42 -892)"><image width="21" height="20" transform="translate(42 892)" xlink:href="data:image/png;base64,"/></g></g></svg>
                  span.meta__text Стрижка
                li.meta__item
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="23" height="21" viewBox="0 0 23 21"><g><g transform="translate(-171 -890)"><image width="23" height="21" transform="translate(171 890)" xlink:href="data:image/png;base64,"/></g></g></svg>
                  span.meta__text Бритье
                li.meta__item
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="45" height="12" viewBox="0 0 45 12"><g><g transform="translate(-30 -937)"><image width="45" height="12" transform="translate(30 937)" xlink:href="data:image/png;base64,"/></g></g></svg>
                  span.meta__text Усы
          .our-works__wrap
            .our-works__item
              .our-works__picture
                picture
                  source(media="(min-width: 768px)", srcset="assets/i/our-works/2-tablet.jpg")
                  img.our-works__image(src="assets/i/our-works/2-mobile.jpg" alt="")

              .our-works__content
                h3.our-works__title Саша Мальцев
                p.our-works__text Попросил омолодить и омолодили! Кто теперь скажет, что мне 45?
                ul.our-works__meta.meta
                  li.meta__item
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="21" height="20" viewBox="0 0 21 20"><g><g transform="translate(-42 -892)"><image width="21" height="20" transform="translate(42 892)" xlink:href="data:image/png;base64,"/></g></g></svg>
                    span.meta__text Стрижка

            .our-works__item
              .our-works__picture
                picture
                  source(media="(min-width: 768px)", srcset="assets/i/our-works/3-tablet.jpg")
                  img.our-works__image(src="assets/i/our-works/3-mobile.jpg" alt="")

              .our-works__content
                h3.our-works__title Владимир Иваныч
                p.our-works__text К зимнему сезону — готов!
                ul.our-works__meta.meta
                  li.meta__item
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="23" height="21" viewBox="0 0 23 21"><g><g transform="translate(-171 -890)"><image width="23" height="21" transform="translate(171 890)" xlink:href="data:image/png;base64,"/></g></g></svg>
                    span.meta__text Бритье
                  li.meta__item
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="45" height="12" viewBox="0 0 45 12"><g><g transform="translate(-30 -937)"><image width="45" height="12" transform="translate(30 937)" xlink:href="data:image/png;base64,"/></g></g></svg>
                    span.meta__text Усы

            .our-works__item
              .our-works__picture
                picture
                  source(media="(min-width: 768px)", srcset="assets/i/our-works/4-tablet.jpg")
                  img.our-works__image(src="assets/i/our-works/4-mobile.jpg" alt="")

              .our-works__content
                h3.our-works__title Винстон Синий
                p.our-works__text Как только заростаю и волосы начинают мешать видеть — бегом в Бородинский!
                ul.our-works__meta.meta
                  li.meta__item
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="21" height="20" viewBox="0 0 21 20"><g><g transform="translate(-42 -892)"><image width="21" height="20" transform="translate(42 892)" xlink:href="data:image/png;base64,"/></g></g></svg>
                    span.meta__text Стрижка
        //.our-works__list
    include ../includes/price-list.pug
  //page-main

  include ../includes/page-footer.pug
  |
  |
block comment-scripts
block scripts
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js")
  script(src="assets/libs/jquery-easy-scroll/jquery.easeScroll.js")
  script(src="assets/js/main.js")

